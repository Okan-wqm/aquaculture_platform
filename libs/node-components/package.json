{
  "name": "@aquaculture/node-components",
  "version": "1.0.0",
  "description": "Shared ReactFlow node components for aquaculture platform",
  "main": "dist/index.cjs",
  "module": "dist/index.mjs",
  "types": "dist/types/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/index.mjs",
      "require": "./dist/index.cjs",
      "types": "./dist/types/index.d.ts"
    },
    "./umd": "./dist/aquaculture-nodes.umd.js"
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "vite build",
    "build:watch": "vite build --watch",
    "typecheck": "tsc --noEmit"
  },
  "peerDependencies": {
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reactflow": "^11.11.4"
  },
  "devDependencies": {
    "@types/react": "^18.2.0",
    "@types/react-dom": "^18.2.0",
    "@vitejs/plugin-react": "^4.2.1",
    "typescript": "~5.3.0",
    "vite": "^5.4.21",
    "vite-plugin-dts": "^3.7.0"
  },
  "keywords": [
    "reactflow",
    "aquaculture",
    "scada",
    "nodes",
    "components"
  ],
  "license": "UNLICENSED"
}
