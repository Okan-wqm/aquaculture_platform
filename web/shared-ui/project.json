{
  "name": "shared-ui",
  "$schema": "../../node_modules/nx/schemas/project-schema.json",
  "sourceRoot": "web/shared-ui/src",
  "projectType": "library",
  "tags": ["scope:web", "type:ui-lib"],
  "targets": {
    "build": {
      "executor": "nx:run-commands",
      "outputs": ["{projectRoot}/dist"],
      "options": {
        "cwd": "web/shared-ui",
        "command": "npm run build"
      }
    },
    "lint": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "web/shared-ui",
        "command": "npm run lint"
      }
    },
    "test": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "web/shared-ui",
        "command": "npm run test"
      }
    },
    "storybook": {
      "executor": "nx:run-commands",
      "options": {
        "cwd": "web/shared-ui",
        "command": "npm run storybook"
      }
    }
  }
}
