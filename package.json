{
  "name": "aquaculture-platform",
  "version": "1.0.0",
  "description": "Enterprise Multi-Tenant Aquaculture Platform with Microservices Architecture",
  "license": "MIT",
  "private": true,
  "workspaces": [
    "apps/*",
    "libs/*",
    "platform/libs/*",
    "web/shared-ui",
    "web/shell",
    "web/modules/*"
  ],
  "scripts": {
    "dev": "nx run-many --target=serve --all",
    "dev:backend": "nx run-many --target=serve --projects=gateway-api,farm-service,sensor-service,auth-service,alert-engine,billing-service,hr-service,notification-service",
    "dev:web": "nx run-many --target=serve --projects=shell,dashboard,farm-module,process-editor,admin-panel",
    "build": "nx affected --target=build",
    "build:all": "nx run-many --target=build --all",
    "build:web": "nx run-many --target=build --projects=shared-ui,shell,dashboard,farm-module,process-editor,admin-panel",
    "test": "nx affected --target=test",
    "test:all": "nx run-many --target=test --all",
    "lint": "nx affected --target=lint",
    "lint:all": "nx run-many --target=lint --all",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "format:check": "prettier --check \"**/*.{ts,tsx,js,jsx,json,md}\"",
    "type-check": "tsc --noEmit",
    "affected:graph": "nx affected:graph",
    "graph": "nx graph",
    "prepare": "husky install"
  },
  "dependencies": {
    "@apollo/gateway": "^2.12.1",
    "@apollo/server": "^4.12.2",
    "@apollo/subgraph": "^2.12.1",
    "@nestjs/apollo": "^12.2.2",
    "@nestjs/common": "^10.3.0",
    "@nestjs/config": "^3.3.0",
    "@nestjs/core": "^10.3.0",
    "@nestjs/cqrs": "^10.2.8",
    "@nestjs/event-emitter": "^3.0.1",
    "@nestjs/graphql": "^12.2.2",
    "@nestjs/jwt": "^11.0.1",
    "@nestjs/passport": "^11.0.5",
    "@nestjs/platform-express": "^10.4.20",
    "@nestjs/platform-socket.io": "^10.4.20",
    "@nestjs/schedule": "^6.0.1",
    "@nestjs/terminus": "^11.0.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.4.20",
    "@types/amqplib": "^0.10.8",
    "@types/nodemailer": "^7.0.4",
    "@types/passport-jwt": "^4.0.1",
    "@types/pdfkit": "^0.17.4",
    "ajv": "^8.17.1",
    "ajv-formats": "^3.0.1",
    "amqplib": "^0.10.9",
    "bcryptjs": "^3.0.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.3",
    "graphql": "^16.12.0",
    "graphql-scalars": "^1.25.0",
    "graphql-type-json": "^0.3.2",
    "helmet": "^7.2.0",
    "ioredis": "^5.8.2",
    "minio": "^8.0.6",
    "modbus-serial": "^8.0.23",
    "mqtt": "^5.14.1",
    "multer": "^2.0.2",
    "nats": "^2.29.3",
    "node-opcua": "^2.159.0",
    "nodemailer": "^7.0.10",
    "nodes7": "^0.3.18",
    "passport": "^0.7.0",
    "passport-jwt": "^4.0.1",
    "pdfkit": "^0.17.2",
    "pg": "^8.16.3",
    "prom-client": "^15.1.3",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "reflect-metadata": "^0.2.2",
    "rxjs": "^7.8.1",
    "socket.io": "^4.8.3",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.27",
    "uuid": "^9.0.1",
    "vite-plugin-svgr": "^4.5.0"
  },
  "devDependencies": {
    "@nestjs/testing": "^10.4.20",
    "@nx/eslint": "18.0.0",
    "@nx/eslint-plugin": "18.0.0",
    "@nx/jest": "18.0.0",
    "@nx/js": "18.0.0",
    "@nx/nest": "18.0.0",
    "@nx/node": "18.0.0",
    "@nx/react": "18.0.0",
    "@nx/vite": "18.0.0",
